{
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"rootDir": "./src",
		"baseUrl": "./",
		"outDir": "./dist",
		"noImplicitAny": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"newLine": "lf"
	},
	"include": ["./src/**/*"]
}
